function [registered, matrix] = colourMatrix(fullFileName)
    I = imread("./images1/org_1.png");
    output = [];

    image=loadImage(fullFileName);
    centers= findCircles(image);
    image = imgaussfilt(image,2);
    registered = correctImage(image, centers, I);
    resultMatrix = findColours(registered);
    matrix = [output; join(resultMatrix)];
end

